<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_stock_quantity_history_inherit" model="ir.ui.view">
        <field name="name">stock.quantity.history.form.inherit.mob</field>
        <field name="model">stock.quantity.history</field>
        <field name="inherit_id" ref="stock.view_stock_quantity_history"/>
        <field name="arch" type="xml">
            <form position="replace">
                <form string="Inventory Report by Date and Location">
                    <group>
                        <field name="location_id" required="1"/>
                        <field name="start_date" required="1"/>
                        <field name="end_date" required="1"/>
                    </group>
                    <footer>
                        <button name="open_at_date" string="Confirm" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </form>
        </field>
    </record>
</odoo> 